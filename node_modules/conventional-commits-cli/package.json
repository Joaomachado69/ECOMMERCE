  },
  "bugs": {
    "url": "https://github.com/pasenidis/conventional-commits-cli/issues"
  },
  "homepage": "https://github.com/pasenidis/conventional-commits-cli#readme",
  "dependencies": {
    "@octokit/auth-basic": "1.4.1",
    "@octokit/rest": "17.1.1",
    "boxen": "4.2.0",
    "chalk": "3.0.0",
    "clear": "0.1.0",
    "clui": "0.3.6",
    "configstore": "5.0.1",
